{
  "name": "material-domain",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "dep:upgrade": "npx npm-check-updates -u && yarn install",
    "eject": "react-scripts eject",
    "lint": "eslint --fix src/**/*.ts src/**/*.tsx",
    "pretty": "prettier --write \"**/*.+(ts|tsx|css)\"",
    "start": "react-scripts start",
    "starts": "HTTPS=true SSL_CRT_FILE=~/localhost+1.pem SSL_KEY_FILE=~/localhost+1-key.pem react-scripts start",
    "test": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "@mui/styles": "^5.0.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.4.4",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1"
  }
}
