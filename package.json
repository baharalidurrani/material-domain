{
  "name": "material-domain",
  "private": true,
  "version": "6.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist/",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "yarn clean && yarn build && yarn preview",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "pretty": "prettier --write \"src/**/*.+(ts|tsx|js|jsx|css|scss)\"",
    "dep:update": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.14",
    "@mui/lab": "^5.0.0-alpha.169",
    "@mui/material": "^5.15.14",
    "@reduxjs/toolkit": "^2.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "tss-react": "^4.9.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.68",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-pwa": "^0.19.6"
  }
}
